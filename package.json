{
  "name": "outline-power-web-clipper",
  "version": "1.0.0",
  "description": "강력한 기능을 갖춘 Outline Wiki 웹 클리퍼",
  "author": "choyunsung",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/choyunsung/outline-web-clipper"
  },
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "package": "node scripts/package.js",
    "package-old": "npm run build && cd dist && bestzip ../releases/outline-clipper.zip *",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mozilla/readability": "0.6.0",
    "axios": "^1.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turndown": "^7.1.2",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.251",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/turndown": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "bestzip": "^2.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
